*perl-ext-common:      default,url-select,clipboard,keyboard-select,bell-command
*keysym.M-Escape:      perl:keyboard-select:activate
*keysym.M-s:           perl:keyboard-select:search
*keysym.M-u:           perl:url-select:select_next
*clipboard.autocopy: true
*url-select.launcher: /usr/bin/xdg-open
*url-select.underline: true
*scrollTtyOutput: false
*scrollWithBufffer: true
*scrollTtyKeypress: true
kuake.kuake.hotkey: F12
kuake.bell-command: twmnc --pos bottom_left --content 'You probably have a message' --size 26

*urgentOnBell: True

*secondaryScreen: 1
*secondaryScroll: 0

*foreground: #00eeff
*background: #101010
*color0: #101010
*color1: #f13a21
*color2: #93f91d
*color3: #ffd00a
*color4: #004f9e
*color5: #ec0048
*color6: #2aa7e7
*color7: #f2f2f2
*color8: #1d202f
*color9: #ff361e
*color10: #ffc005
*color11: #93ff00
*color12: #0071ff
*color13: #ef0051
*color14: #4bb8fd
*color15: #a020f0
*transparent: true
*shading: 45
*fading: 20
*font: xft:Bitstream\ Vera\ Sans\ Mono:style=roman:pixelsize=15
*scrollBar: false
